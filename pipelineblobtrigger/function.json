{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "myblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "inputcontainer",
      "connection": "taskinputs_STORAGE"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "$return",
      "path": "outputcontainer/outputdata",
      "connection": "taskoutputs_STORAGE"
    }
  ]
}
